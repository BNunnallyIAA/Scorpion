
{ # Application
   name 'TestApplication'        # Manditory
   {
       minDevVersion 7           # Manditory
   	   targetDevVersion 7        # Manditory
	   versionNumber '0.0.7'     # Manditory
   }
   
   { # Singing configs
       debug false 
   }

   { # Logging
       log true
	   logPrecedence 7
	   logFile ('file.log') # output file
   }

   { # dependencies
        compile ('lib/test-lib.tz')
   }

   { # security permissions
       permission 'scorpion.security.permissions.READ_FILES'
   }
}

